<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <appSettings>
    <add key="BaseUrl" value="http://dlcs.azurewebsites.net" xdt:Transform="SetAttributes" xdt:Locator="Match(name)" />
    <add key="Vocab" value="http://dlcs.azurewebsites.net/vocab#" xdt:Transform="SetAttributes" xdt:Locator="Match(name)" />
  </appSettings>
  <system.web>
    <compilation xdt:Transform="RemoveAttributes(debug)" />
  </system.web>
</configuration>
